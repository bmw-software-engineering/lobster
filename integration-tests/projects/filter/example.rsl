package exampletwo

type System_Requirement
{
  text                       String
}

type Software_Requirement
{
  text                String
  trace_trlc optional System_Requirement [1 .. *]
}

checks System_Requirement {
   len(text) >= 10, warning "this is a bit short", text
}

checks Software_Requirement {
   len(text) >= 10, warning "this is a bit short", text
}
